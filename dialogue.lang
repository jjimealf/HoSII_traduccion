	<string name="dialogue.mercenary.join2">¡Responderemos a tu llamado a las armas!</string>
	<string name="dialogue.mercenary.archon.join1">¡Lidera el camino!</string>
	<string name="dialogue.mercenary.archon.join2">¡Tú vas, nosotros seguimos!</string>

